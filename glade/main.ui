<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.18"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkTreeStore" id="taskinfo_ts"/>
  <object class="GtkTreeStore" id="server_ts">
    <columns>
      <!-- column-name server_status -->
      <column type="gchararray"/>
      <!-- column-name server_name -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkTreeStore" id="tasklist_ts">
    <columns>
      <!-- column-name task_gid -->
      <column type="gchararray"/>
      <!-- column-name task_status -->
      <column type="gchararray"/>
      <!-- column-name task_name -->
      <column type="gchararray"/>
      <!-- column-name task_dir -->
      <column type="gchararray"/>
      <!-- column-name task_size -->
      <column type="gchararray"/>
      <!-- column-name task_down_speed -->
      <column type="gchararray"/>
      <!-- column-name task_up_speed -->
      <column type="gchararray"/>
      <!-- column-name task_connections -->
      <column type="gint"/>
    </columns>
  </object>
  <object class="GtkWindow" id="main_window">
    <property name="width_request">700</property>
    <property name="height_request">500</property>
    <property name="title" translatable="yes">Yaner - Faster than a flash of lightning</property>
    <property name="icon_name">emblem-downloads</property>
    <signal name="destroy" handler="on_quit_action_activate"/>
    <child>
      <object class="GtkVBox" id="main_vbox">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkMenuBar" id="main_menu_bar">
            <property name="visible">True</property>
            <child>
              <object class="GtkMenuItem" id="file_menu">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_File</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="file_sub_menu">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="task_new_item">
                        <property name="label">gtk-new</property>
                        <property name="visible">True</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                        <accelerator key="n" signal="activate" modifiers="GDK_CONTROL_MASK"/>
                        <child type="submenu">
                          <object class="GtkMenu" id="task_new_menu">
                            <property name="visible">True</property>
                            <child>
                              <object class="GtkMenuItem" id="task_new_normal_item">
                                <property name="visible">True</property>
                                <property name="use_action_appearance">True</property>
                                <property name="related_action">task_new_normal_action</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkMenuItem" id="task_new_bt_item">
                                <property name="visible">True</property>
                                <property name="use_action_appearance">True</property>
                                <property name="related_action">task_new_bt_action</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkMenuItem" id="task_new_metalink_item">
                                <property name="visible">True</property>
                                <property name="use_action_appearance">True</property>
                                <property name="related_action">task_new_metalink_action</property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorMenuItem" id="separator1_item">
                        <property name="visible">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="quit_item">
                        <property name="visible">True</property>
                        <property name="related_action">quit_action</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                        <accelerator key="q" signal="activate" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="task_menu">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Task</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="task_sub_menu">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="task_start_item">
                        <property name="visible">True</property>
                        <property name="use_action_appearance">True</property>
                        <property name="related_action">task_start_action</property>
                        <property name="use_stock">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="task_pause_item">
                        <property name="visible">True</property>
                        <property name="use_action_appearance">True</property>
                        <property name="related_action">task_pause_action</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="task_remove_item">
                        <property name="visible">True</property>
                        <property name="use_action_appearance">True</property>
                        <property name="related_action">task_remove_action</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="help_menu">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Help</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="help_sub_menu">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="about_item">
                        <property name="visible">True</property>
                        <property name="related_action">about_action</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkToolbar" id="toolbar">
            <property name="visible">True</property>
            <child>
              <object class="GtkMenuToolButton" id="task_new_tool_item">
                <property name="visible">True</property>
                <property name="use_action_appearance">False</property>
                <property name="related_action">task_new_normal_action</property>
                <property name="label" translatable="yes">Add new task</property>
                <property name="stock_id">gtk-add</property>
                <property name="menu">task_new_menu</property>
              </object>
              <packing>
                <property name="expand">False</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="task_remove_toolbutton">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">task_remove_action</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="separatortoolitem1">
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="task_start_toolbutton">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">task_start_action</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="task_pause_toolbutton">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">task_pause_action</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="separatortoolitem2">
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="about_toolbutton">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">about_action</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="quit_toolbutton">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">quit_action</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkHPaned" id="main_hpaned">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <child>
              <object class="GtkScrolledWindow" id="server_sw">
                <property name="width_request">150</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="hscrollbar_policy">automatic</property>
                <property name="vscrollbar_policy">automatic</property>
                <child>
                  <object class="GtkTreeView" id="server_tv">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="model">server_ts</property>
                    <property name="headers_visible">False</property>
                    <property name="headers_clickable">False</property>
                    <property name="search_column">1</property>
                    <child>
                      <object class="GtkTreeViewColumn" id="server_tc">
                        <property name="title">Servers</property>
                        <child>
                          <object class="GtkCellRendererPixbuf" id="server_status_renderer"/>
                          <attributes>
                            <attribute name="stock-id">0</attribute>
                          </attributes>
                        </child>
                        <child>
                          <object class="GtkCellRendererText" id="server_name_renderer"/>
                          <attributes>
                            <attribute name="markup">1</attribute>
                          </attributes>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="resize">False</property>
                <property name="shrink">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkVPaned" id="main_vpaned">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkScrolledWindow" id="tasklist_sw">
                    <property name="height_request">150</property>
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="hscrollbar_policy">automatic</property>
                    <property name="vscrollbar_policy">automatic</property>
                    <child>
                      <object class="GtkTreeView" id="tasklist_tv">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="rules_hint">True</property>
                        <property name="enable_grid_lines">vertical</property>
                        <child>
                          <object class="GtkTreeViewColumn" id="gid_tc">
                            <property name="resizable">True</property>
                            <property name="title">GID</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_gid_renderer"/>
                              <attributes>
                                <attribute name="text">0</attribute>
                              </attributes>
                            </child>
                            <child>
                              <object class="GtkCellRendererPixbuf" id="task_status_renderer"/>
                              <attributes>
                                <attribute name="stock-id">1</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="name_tc">
                            <property name="resizable">True</property>
                            <property name="title">Name</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_name_renderer"/>
                              <attributes>
                                <attribute name="text">2</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="progress_tc">
                            <property name="resizable">True</property>
                            <property name="title">Progress</property>
                            <child>
                              <object class="GtkCellRendererProgress" id="task_progress_renderer"/>
                              <attributes>
                                <attribute name="text">4</attribute>
                                <attribute name="value">3</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="size_tc">
                            <property name="resizable">True</property>
                            <property name="title">Size</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_size_renderer"/>
                              <attributes>
                                <attribute name="text">5</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="down_speed_tc">
                            <property name="resizable">True</property>
                            <property name="title">DownSpeed</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_down_speed_renderer"/>
                              <attributes>
                                <attribute name="text">6</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="up_speed_tc">
                            <property name="resizable">True</property>
                            <property name="title">UpSpeed</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_up_speed_renderer"/>
                              <attributes>
                                <attribute name="text">7</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTreeViewColumn" id="connections_tc">
                            <property name="resizable">True</property>
                            <property name="title">Connections</property>
                            <child>
                              <object class="GtkCellRendererText" id="task_connections_renderer"/>
                              <attributes>
                                <attribute name="text">8</attribute>
                              </attributes>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="resize">True</property>
                    <property name="shrink">True</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkScrolledWindow" id="taskinfo_sw">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="hscrollbar_policy">automatic</property>
                    <property name="vscrollbar_policy">automatic</property>
                    <child>
                      <object class="GtkTreeView" id="taskinfo_tv">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <property name="model">taskinfo_ts</property>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="resize">True</property>
                    <property name="shrink">True</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="resize">True</property>
                <property name="shrink">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkStatusbar" id="statusbar">
            <property name="visible">True</property>
            <property name="spacing">2</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">3</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkAction" id="about_action">
    <property name="stock_id">gtk-about</property>
    <signal name="activate" handler="on_about_action_activate" object="about_dialog"/>
  </object>
  <object class="GtkAction" id="quit_action">
    <property name="stock_id">gtk-quit</property>
    <signal name="activate" handler="on_quit_action_activate"/>
  </object>
  <object class="GtkAboutDialog" id="about_dialog">
    <property name="border_width">5</property>
    <property name="title" translatable="yes">About Yaner</property>
    <property name="window_position">center-on-parent</property>
    <property name="destroy_with_parent">True</property>
    <property name="icon_name">emblem-downloads</property>
    <property name="type_hint">normal</property>
    <property name="has_separator">False</property>
    <property name="program_name">Yaner</property>
    <property name="copyright" translatable="yes">Copyright &#xA9; 2010 Iven Day(Xu Lijian)</property>
    <property name="website">http://www.kissuki.com/</property>
    <property name="authors">Iven Day(Xu Lijian) &lt;ivenvd@gmail.com&gt;</property>
    <property name="logo_icon_name">emblem-downloads</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="about_dialog_vbox">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child>
          <placeholder/>
        </child>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="about_dialog_action_area">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkStatusIcon" id="statusicon">
    <property name="icon_name">emblem-downloads</property>
  </object>
  <object class="GtkAction" id="task_new_normal_action">
    <property name="label">HTTP/FTP/BT Magnet</property>
    <signal name="activate" handler="on_task_new_action_activate" object="task_new_normal_action"/>
  </object>
  <object class="GtkAction" id="task_new_bt_action">
    <property name="label">BitTorrent</property>
    <signal name="activate" handler="on_task_new_action_activate" object="task_new_bt_action"/>
  </object>
  <object class="GtkAction" id="task_new_metalink_action">
    <property name="label">Metalink</property>
    <signal name="activate" handler="on_task_new_action_activate" object="task_new_metalink_action"/>
  </object>
  <object class="GtkAction" id="task_remove_action">
    <property name="label">Remove</property>
    <property name="stock_id">gtk-remove</property>
    <signal name="activate" handler="on_task_remove_action_activate"/>
  </object>
  <object class="GtkAction" id="task_pause_action">
    <property name="label">Pause</property>
    <property name="stock_id">gtk-media-pause</property>
    <signal name="activate" handler="on_task_pause_action_activate"/>
  </object>
  <object class="GtkAction" id="task_start_action">
    <property name="label">Start</property>
    <property name="stock_id">gtk-media-play</property>
    <signal name="activate" handler="on_task_start_action_activate"/>
  </object>
</interface>
